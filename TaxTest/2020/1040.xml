<?xml version="1.0" encoding="utf-8" ?>
<Form>
  <Line Number="1" Name="W-2Income" Calc="SUM(FormW-2.Wages)" />
  <Line Number="2b" Name="TaxableInterest" />
  <Line Number="3b" Name="OrdinaryDividends" />
  <Line Number="7" Name="CapitalGain" />
  <Line Number="9" Name="TotalIncome" Calc="Form1040.Line1 + Form1040.Line2b + Form1040.Line3b + Form1040.Line7" />
  <!-- TODO: line 10 adjustments -->
  <Line Number="11" Name="AGI" Calc="Form1040.TotalIncome" />
  <Line Number="12" Name="Deductions" />
  <Line Number="13" Name="QualifiedBusinessIncomeDeduction" />
  <Line Number="14" Name="AllDeductions" Calc="Form1040.Line12 + Form1040.Line13" />
  <Line Number="15" Name="TaxableIncome" Calc="MAX(0, Form1040.Line11 - Form1040.Line14)" />
</Form>